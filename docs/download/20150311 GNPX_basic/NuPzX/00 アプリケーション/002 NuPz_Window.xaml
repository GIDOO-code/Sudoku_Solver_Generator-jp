<Window x:Class="GNPZ_sdk.NuPz_Win"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:extToolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit/extended" 
        xmlns:my="http://schemas.microsoft.com/wpf/2008/toolkit" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"  
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        
        mc:Ignorable="d"
        
        Title="NuPz_Window" 
        Loaded="Window_Loaded" Background="#FF181846" ShowInTaskbar="False" WindowStyle="None" 
        ShowActivated="True" ResizeMode="NoResize" Unloaded="Window_Unloaded" BorderBrush="Black" 
        MouseDown="Window_MouseDown" Width="954" Height="523" >
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Styles/DataGridStyle.xaml"></ResourceDictionary>
                <!--
                <ResourceDictionary Source="/Styles/ListBoxStyle1.xaml"></ResourceDictionary>
-->
                <ResourceDictionary Source="/Styles/TabControlStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Styles/TextBlockStyle.xaml"></ResourceDictionary>

                <ResourceDictionary>
                    <Style TargetType="{x:Type ComboBox}">
                        <Style.Triggers>
                            <Trigger Property="IsKeyboardFocusWithin" Value="true">
                                <Setter Property="Background" Value="Navy"/>
                            </Trigger>
                        </Style.Triggers>
                    </Style>
                </ResourceDictionary>

            </ResourceDictionary.MergedDictionaries>

        </ResourceDictionary>
    </Window.Resources>

    <Grid VerticalAlignment="Stretch" Margin="3,0,0,0" PreviewKeyDown="Grid_PreviewKeyDown">
        <Grid.Background>
            <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                <GradientStop Color="#FF302A3E" />
                <GradientStop Color="#FF5E5E80" Offset="1" />
                <GradientStop Color="#BE232136" Offset="0.43"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Button Content="終 了" FontFamily="MS Gothic" FontSize="12" Foreground="White" Height="29" HorizontalAlignment="Left" Margin="841,480,0,0" x:Name="appExit" VerticalAlignment="Top" Width="56" Click="appExit_Click" Style="{Binding}">
            <Button.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0.8" />
                    <GradientStop Color="#FF7F7E85" Offset="0.05" />
                    <GradientStop Color="#EA575757" Offset="0.55" />
                </LinearGradientBrush>
            </Button.Background>
        </Button>
        <Label Content="LbShortMes" Height="28" HorizontalAlignment="Left" Margin="185,34,0,0" x:Name="LbShortMes" VerticalAlignment="Top" Foreground="Red" RenderTransformOrigin="0.5,0.5" Visibility="Hidden" >
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.743"/>
                    <TranslateTransform X="2.545" Y="0.509"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>

        <Label Content="難易度" FontFamily="MS Gothic" FontSize="14" Foreground="#FFFEFFFF" HorizontalAlignment="Left" Margin="270,91,0,0" x:Name="label1" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" >
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.743"/>
                    <TranslateTransform X="2.11" Y="1.537"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <TextBox x:Name="txtProbName" Text="........." Background="#FF0A0A24" Width="269" Margin="86,70,0,0" 
                 FontFamily="MS Gothic" FontSize="14" Foreground="White" Height="19" 
                 HorizontalAlignment="Left"   VerticalAlignment="Top" VerticalContentAlignment="Center" 
                 RenderTransformOrigin="0.5,0.5" KeyUp="txtProbName_KeyUp" >
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.743"/>
                    <TranslateTransform X="2.127" Y="-0.077"/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <TextBox Background="#FF0A0A24" FontFamily="MS Gothic" FontSize="14" Foreground="White" Height="19" HorizontalAlignment="Left" Margin="43,69,0,0" x:Name="txtProbNo" Text="999" TextAlignment="Center" VerticalAlignment="Top" Width="37" VerticalContentAlignment="Center" RenderTransformOrigin="0.5,0.5" HorizontalContentAlignment="Center" >
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.743"/>
                    <TranslateTransform X="2.137" Y="-1.607"/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <Label Content="問題" FontFamily="MS Gothic" FontSize="12" Foreground="#FFFEFFFF" HorizontalAlignment="Left" Margin="9,68,0,0" x:Name="label2" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" >
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.743"/>
                    <TranslateTransform X="2.134" Y="-2.068"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Image x:Name="PB_GBoard" Width="338" Height="338" Margin="18,118,0,0" Stretch="Fill" VerticalAlignment="Top"
               DataContext="{Binding}" MouseLeftButtonDown="PB_GBoard_MouseLeftButtonDown"
               MouseLeftButtonUp="PB_GBoard_MouseLeftButtonUp" HorizontalAlignment="Left" 
               RenderTransformOrigin="0.5,0.5" Source="/GNPX_Basic;component/40 WPFColorPickerLib/Images/ColorSwatchCircle.png" />
        <Label x:Name="lblStepCounter" Content=" セル数 問:.. 解:.. 残:.." FontFamily="MS Gothic" FontSize="12" 
               Foreground="WhiteSmoke" HorizontalAlignment="Left" Margin="17,94,0,0" VerticalAlignment="Top" 
               RenderTransformOrigin="0.5,0.5" >
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.743"/>
                    <TranslateTransform X="1.842" Y="-1.149"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Image Height="36" HorizontalAlignment="Left" Margin="17,117,0,0" x:Name="GnumericPad" Stretch="Fill" 
               VerticalAlignment="Top" Width="36" Visibility="Visible" MouseMove="GnumericPad_MouseMove" 
               MouseUp="GnumericPad_MouseUp" RenderTransformOrigin="0.5,0.5" />
        <TabControl x:Name="tabCtrlMode" Background="Black" 
    		Height="445" HorizontalAlignment="Left" Margin="376,28,0,0"
    		VerticalAlignment="Top" Width="557" SelectionChanged="tabCtrlMode_SelectionChanged">
            <TabItem x:Name="tabAFileIO" Header="ファイル" Style="{DynamicResource ResourceKey=styleTabItemTop}" Width="75" FontSize="12" FontFamily="MS Mincho" >
                <Grid Width="551" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="94*"/>
                        <ColumnDefinition Width="57*"/>
                        <ColumnDefinition Width="360*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.Background>
                        <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                            <GradientStop Color="#FF3A3948" Offset="0" />
                            <GradientStop Color="#FF65646F" Offset="1" />
                        </LinearGradientBrush>
                    </Grid.Background>
                    <Button Content="ファイル保存" Foreground="White" Height="25" HorizontalAlignment="Left" Margin="42,156,0,0" x:Name="btnFileOutputQ" VerticalAlignment="Top" Width="100" Click="btnFileOutputQ_Click" FontSize="12" Grid.ColumnSpan="2">
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFB0B0CA" Offset="0" />
                                <GradientStop Color="#FF0D0D1A" Offset="1" />
                                <GradientStop Color="#FF434399" Offset="0.508" />
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <TextBox Height="39" HorizontalAlignment="Left" Margin="54,61,0,0" x:Name="txtFileName" Text="問題ファイル" VerticalAlignment="Top" Width="299" TextWrapping="WrapWithOverflow" Grid.ColumnSpan="3" FontSize="12" />
                    <Button Content="お気に入り保存" Foreground="White" Height="25" HorizontalAlignment="Left" Margin="131,156,0,0" x:Name="btnFavfileOutputQ" VerticalAlignment="Top" Width="100" Click="btnFavfileOutputQ_Click" FontSize="12" Grid.Column="2">
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFCACACA" Offset="0" />
                                <GradientStop Color="#FF6F6F85" Offset="1" />
                                <GradientStop Color="#FF4C31A0" Offset="0.508" />
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <Rectangle Height="3" HorizontalAlignment="Left" Margin="31,136,0,0" x:Name="grid1" VerticalAlignment="Top" Width="386" Fill="#FF0000C5" Grid.ColumnSpan="3" />
                    <Label Content="全問題を保存" Foreground="White" Margin="48,183,0,0" x:Name="label14" Height="28" VerticalAlignment="Top" HorizontalAlignment="Left" Width="94" FontSize="12" Grid.ColumnSpan="2" />
                    <Label Content="表示問題を保存" Foreground="White" Height="28" HorizontalAlignment="Left" Margin="142,183,0,0" x:Name="label15" VerticalAlignment="Top" FontSize="12" Grid.Column="2" />
                    <CheckBox Content="フォーマット：81数字・一列" Foreground="White" Height="16" Margin="66,222,102,0" x:Name="cbxFile81Nocsv" IsChecked="True" VerticalAlignment="Top" FontSize="12" Grid.ColumnSpan="3" VerticalContentAlignment="Center" />
                    <CheckBox Content="難易度順にソートして出力" Foreground="White" Height="16" Margin="66,238,0,0" x:Name="cbxSolutionSort" VerticalAlignment="Top" HorizontalAlignment="Left" Width="176" FontSize="12" Grid.ColumnSpan="3" VerticalContentAlignment="Center" />
                    <CheckBox Content="解を出力" Foreground="White" Margin="66,250,0,0" x:Name="cbxProbSolSetOutput" Height="24" VerticalAlignment="Top" HorizontalAlignment="Left" Width="89" Checked="cbxProbSolSetOutput_Checked" Unchecked="cbxProbSolSetOutput_Checked" FontSize="12" Grid.ColumnSpan="3" VerticalContentAlignment="Center" />
                    <CheckBox Content="ファイルが存在するとき追記" Foreground="White" Height="20" Margin="66,204,0,0" x:Name="cbxAppend" VerticalAlignment="Top" IsChecked="True" HorizontalAlignment="Left" Width="184" FontSize="12" Grid.ColumnSpan="3" VerticalContentAlignment="Center" />
                    <CheckBox Content="初期状態に戻して入力" Foreground="White" Height="20" IsChecked="True" Margin="66,99,102,0" x:Name="cbxProbInitialSet" VerticalAlignment="Top" FontSize="12" Grid.ColumnSpan="3" VerticalContentAlignment="Center" />
                    <Button x:Name="btnFileInputQ" Content="ファイル入力"  Width="100" Height="25" VerticalAlignment="Top" Margin="42,31,0,0"
    					Foreground="White" HorizontalAlignment="Left" FontSize="12" Grid.ColumnSpan="2" Click="btnFileInputQ_Click" >
                        <Button.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFA3A3A3" Offset="0" />
                                <GradientStop Color="Black" Offset="1" />
                            </LinearGradientBrush>
                        </Button.Background>
                    </Button>
                    <CheckBox Content="解法リスト出力" Foreground="Gray" Height="17" HorizontalAlignment="Left" Margin="82,271,0,0" x:Name="cbxProbSolSetOutput2" VerticalAlignment="Top" Width="122" IsEnabled="False" FontSize="12" Grid.ColumnSpan="3" VerticalContentAlignment="Center" />
                </Grid>
            </TabItem>

            <TabItem x:Name="tabAAnalyze" Header="解析" Style="{DynamicResource ResourceKey=styleTabItemTop}" Width="45" FontFamily="MS Mincho" FontSize="12">
                <Grid HorizontalAlignment="Left" Width="550">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="446*"/>
                        <ColumnDefinition Width="67*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.Background>
                        <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                            <GradientStop Color="#FF3A3948" Offset="0" />
                            <GradientStop Color="#FF65646F" Offset="1" />
                        </LinearGradientBrush>
                    </Grid.Background>
                    <TabControl Height="396" HorizontalAlignment="Left" Margin="10,6,0,0" x:Name="tabSolver" VerticalAlignment="Top" Width="530"
                                Background="#FFEFEFEF" BorderBrush="{x:Null}" SelectionChanged="tabSolver_SelectionChanged" Grid.ColumnSpan="2">
                        <TabItem x:Name="TC_SDKAnalyzer" Header="解析"  Style="{DynamicResource ResourceKey=styleTabItemTopSmall}" Width="40" HorizontalAlignment="Left" FontFamily="MS Mincho" FontSize="12">
                            <Grid x:Name="AutoSolNoDisp" Width="523" Margin="0,0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="5*"/>
                                    <ColumnDefinition Width="42*"/>
                                    <ColumnDefinition Width="193*"/>
                                </Grid.ColumnDefinitions>
                                <Grid.Background>
                                    <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                                        <GradientStop Color="#FF3A3948" Offset="0" />
                                        <GradientStop Color="#FF65646F" Offset="1" />
                                    </LinearGradientBrush>
                                </Grid.Background>
                                <Label Content="手  法" FontSize="12" Foreground="White" Height="23" HorizontalAlignment="Left" Margin="33,135,0,0" x:Name="label16" VerticalAlignment="Top" Grid.Column="1" />
                                <Label Content="適用回数" FontSize="12" Foreground="White" Height="23" HorizontalAlignment="Left" Margin="112,135,0,0" x:Name="label17" VerticalAlignment="Top" Grid.Column="2" />
                                <ScrollViewer HorizontalAlignment="Left" Width="350" VerticalScrollBarVisibility="Auto" Grid.Column="1" Grid.ColumnSpan="2" Margin="0,22,0,232" >
                                    <TextBox Background="#FF0A0A24" Margin="0" x:Name="lblAnalyzerResult" Foreground="#FFF2F2F2" FontFamily="MS Mincho" FontSize="12" HorizontalContentAlignment="Stretch" TextOptions.TextFormattingMode="Display" TextWrapping="WrapWithOverflow" Width="332" HorizontalAlignment="Left" />
                                </ScrollViewer>
                                <ScrollViewer  VerticalScrollBarVisibility="Auto" Grid.Column="1" Grid.ColumnSpan="2" Margin="1,156,162,5" RenderTransformOrigin="0.5,0.5" Width="350">
                                    <ScrollViewer.RenderTransform>
                                        <TransformGroup>
                                            <ScaleTransform ScaleY="-1"/>
                                            <SkewTransform/>
                                            <RotateTransform/>
                                            <TranslateTransform/>
                                        </TransformGroup>
                                    </ScrollViewer.RenderTransform>
                                    <DataGrid AutoGenerateColumns="False" 
                                	x:Name="DGViewMethodCounter" 
                                	Background="#FF0A0A24" 
                                	FontFamily="MS Mincho" 
                                	Foreground="White" 
                                	ItemsSource="{Binding}" 
                                	MinWidth="10" MinHeight="14" 
                                	FontStretch="Normal"
                                	IsSynchronizedWithCurrentItem="True" 
                                	MinRowHeight="14" RowHeight="14" 
                                	CanUserSortColumns="False" 
                                	HeadersVisibility="None" 
                                	MinColumnWidth="12" 
                                	RowBackground="#FF0A0A24" VerticalGridLinesBrush="#FFAFB7EB" 
                                	HorizontalGridLinesBrush="#FFAFB7EB" CanUserResizeColumns="True" CanUserReorderColumns="False" BorderBrush="#FF4F4F72" FrozenColumnCount="2" IsReadOnly="True" SelectionMode="Single" FontSize="12" HorizontalAlignment="Left" Width="332" Grid.IsSharedSizeScope="True" Grid.ColumnSpan="3" Height="204" RenderTransformOrigin="0.5,0.5">
                                        <DataGrid.RenderTransform>
                                            <TransformGroup>
                                                <ScaleTransform ScaleY="-1"/>
                                                <SkewTransform/>
                                                <RotateTransform/>
                                                <TranslateTransform/>
                                            </TransformGroup>
                                        </DataGrid.RenderTransform>
                                        <DataGrid.Columns>
                                            <DataGridTextColumn FontFamily="MS Gothic" Foreground="White" Binding="{Binding methodName}" Width="5*" />
                                            <DataGridTextColumn FontFamily="MS Gothic" Foreground="White" Binding="{Binding count}" Width="1*"/>
                                        </DataGrid.Columns>
                                        <DataGrid.RowStyle>
                                            <Style TargetType="{x:Type DataGridRow}">
                                                <Style.Triggers>
                                                    <Trigger Property="IsSelected" Value="True">
                                                        <Setter Property="Background" Value="#FF0A0A24" />
                                                        <Setter Property="Foreground" Value="White" />
                                                    </Trigger>
                                                    <Trigger Property="IsFocused" Value="True">
                                                        <Setter Property="Background" Value="#FF0A0A24" />
                                                        <Setter Property="Foreground" Value="White" />
                                                    </Trigger>
                                                </Style.Triggers>
                                            </Style>
                                        </DataGrid.RowStyle>
                                    </DataGrid>
                                </ScrollViewer>

                                <Button x:Name="btnSDKAnalyzer" Content="解　析" FontSize="12" Foreground="White" 
    								Margin="278,24,0,0" VerticalAlignment="Top" Width="90" Height="24" HorizontalAlignment="Left"
    								Click="btnSDKAnalyzer_Click" Grid.Column="2">
                                    <Button.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFC1B5B5" Offset="0" />
                                            <GradientStop Color="#FF161D6C" Offset="0.472" />
                                            <GradientStop Color="Black" Offset="1" />
                                        </LinearGradientBrush>
                                    </Button.Background>
                                </Button>
                                <Button Content="解析リセット" FontSize="12" Foreground="White" Height="24" HorizontalAlignment="Left" Margin="278,57,0,0" x:Name="btnAnalyzerReset" VerticalAlignment="Top" Width="90" Click="btnAnalyzerReset_Click" Grid.Column="2">
                                    <Button.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FF7EA17E" Offset="0" />
                                            <GradientStop Color="#FF103810" Offset="1" />
                                            <GradientStop Color="#FF184818" Offset="0.626" />
                                        </LinearGradientBrush>
                                    </Button.Background>
                                </Button>
                                <Button x:Name="btnSDKAnalyzerAuto" Content="自動解析" FontSize="12" Foreground="White" Height="24" 
                                        HorizontalAlignment="Left" Margin="278,103,0,0" VerticalAlignment="Top" 
                                        Width="90" Click="btnSDKAnalyzerAuto_Click" Grid.Column="2">
                                    <Button.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFC1B5B5" Offset="0" />
                                            <GradientStop Color="#FF161D6C" Offset="0.472" />
                                            <GradientStop Color="Black" Offset="1" />
                                        </LinearGradientBrush>
                                    </Button.Background>
                                </Button>
                                <Button Content="全リセット" FontSize="12" Foreground="White" Height="24" HorizontalAlignment="Left" 
                                        Margin="278,136,0,0" x:Name="btnAnalyzerResetAll" VerticalAlignment="Top" Width="90" 
                                        Click="btnAnalyzerResetAll_Click" Grid.Column="2">
                                    <Button.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FF7EA17E" Offset="0" />
                                            <GradientStop Color="#FF103810" Offset="1" />
                                            <GradientStop Color="#FF184818" Offset="0.626" />
                                        </LinearGradientBrush>
                                    </Button.Background>
                                </Button>
                                <Label Content="解析中" FontSize="12" FontWeight="Normal" Foreground="#FFFF4043" HorizontalAlignment="Left" Margin="69,0,0,0" x:Name="Lbl_onAnalyzer" VerticalAlignment="Top" Grid.ColumnSpan="2" Grid.Column="1" />
                                <Label Content="0 sec" FontSize="12" Foreground="#FFEFF0F8" HorizontalAlignment="Right" Margin="0,0,198,345" x:Name="Lbl_onAnalyzerTS" VerticalAlignment="Bottom" FontWeight="Normal" FontFamily="MS Gothic" RenderTransformOrigin="1.45,0.636" HorizontalContentAlignment="Right" Grid.Column="2" Height="21" />
                                <Label Content="ステップ 0" FontSize="12" Foreground="White" Height="23" HorizontalAlignment="Left" Margin="284,6,0,0" x:Name="btnAnalyzerCC" VerticalAlignment="Top" RenderTransformOrigin="0,0.391" Grid.Column="2" Width="84" />
                                <Grid Margin="0,204,7,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="148" Grid.Column="2">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="59*"/>
                                        <ColumnDefinition Width="89*"/>
                                    </Grid.ColumnDefinitions>
                                    <CheckBox Background="White" Content="空き数字を表示" FontSize="11" Foreground="White" Margin="11,-20,0,0" x:Name="chbAnalyze00" IsChecked="True" HorizontalAlignment="Left" Checked="chbAnalyze00_Checked" Unchecked="chbAnalyze00_Checked" VerticalAlignment="Top" FontFamily="MS Gothic" VerticalContentAlignment="Bottom" Grid.ColumnSpan="2" />
                                    <CheckBox Background="White" Content="複数セルを確定" FontSize="11" Foreground="White" Margin="11,0,0,-15" x:Name="MltSolOn" IsChecked="True" VerticalAlignment="Top" HorizontalAlignment="Left" FontFamily="MS Gothic" VerticalContentAlignment="Bottom" Grid.ColumnSpan="2" />
                                    <CheckBox Background="White" Content="難易度設定" FontSize="11" Foreground="White" Margin="11,21,0,-36" x:Name="cbxFileDifficultyLevel" IsThreeState="False" HorizontalAlignment="Left" VerticalAlignment="Top" IsChecked="True" FontFamily="MS Gothic" VerticalContentAlignment="Bottom" Grid.ColumnSpan="2" />

                                </Grid>
                                <Button x:Name="btnSDKAnalyzerPls" Content="+" FontSize="12" Foreground="White" 
                                	Margin="383,28,0,0" VerticalAlignment="Top" Width="21" Height="17" HorizontalAlignment="Left" Grid.Column="2">
                                    <Button.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFC1B5B5" Offset="0" />
                                            <GradientStop Color="#FF161D6C" Offset="0.472" />
                                            <GradientStop Color="Black" Offset="1" />
                                        </LinearGradientBrush>
                                    </Button.Background>
                                </Button>

                            </Grid>
                        </TabItem>
