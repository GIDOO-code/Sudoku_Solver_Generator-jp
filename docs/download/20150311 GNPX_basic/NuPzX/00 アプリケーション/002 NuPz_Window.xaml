                        			Margin="402,27,0,0" VerticalAlignment="Top" Width="90" Height="24" HorizontalAlignment="Left"
                        			Click="btnMltAnlzSearch_Click">
                                    <Button.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFC1B5B5" Offset="0" />
                                            <GradientStop Color="#FF161D6C" Offset="0.472" />
                                            <GradientStop Color="Black" Offset="1" />
