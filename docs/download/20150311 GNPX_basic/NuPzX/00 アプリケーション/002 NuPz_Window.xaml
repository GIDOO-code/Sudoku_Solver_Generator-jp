                                    <ScrollViewer HorizontalAlignment="Left" Width="370" Margin="0,0,-127,0">
                                        <TextBox Background="#FF0A0A24" Margin="0" x:Name="lblAnalyzerResultM" Foreground="#FFF2F2F2" FontFamily="MS Mincho" FontSize="12" HorizontalContentAlignment="Stretch" TextOptions.TextFormattingMode="Display" TextWrapping="WrapWithOverflow" Width="369" ScrollViewer.CanContentScroll="True" />
                                    </ScrollViewer>
                                </Grid>
